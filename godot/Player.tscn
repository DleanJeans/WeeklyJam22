script/source = "extends AudioStreamPlayer2D

var sounds = [load(\"res://assets/screams/FunnyScream.ogg\"), load(\"res://assets/screams/MaleScream.ogg\"), load(\"res://assets/screams/FemaleScream.ogg\")]

var individual_scream
var crowd_scream = load(\"res://assets/screams/Crowd.ogg\")

func choose_random_sound():
	individual_scream = sounds[randi() % sounds.size()]

func play_crowd_screaming():
	if playing: return
	print(\"Crowd Screaming\")
	stream = crowd_scream
	play()

func play_random_individual():
	if playing: return
	stream = individual_scream
	print(\"Individual Screaming\")
	play()

func stop():
	if playing:
		print(\"Screaming Stopped\")
		.stop()"

