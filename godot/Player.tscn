script/source = "extends Timer

onready var player = get_parent()
onready var label = player.get_node(\"TimerLabel\")
onready var jump_duration = player.get_node(\"AnimationPlayer\").get_animation(\"Jump\").get_length()

var jumped = false

func start():
	.start()
	player.toggle_frozen()
	jumped = false

func _process(delta):
	var time = get_time_left()
	if time > 0:
		label.text = String(time + 1).substr(0, 1)
	else:
		label.text = \"\"
	
	if time < jump_duration / 2 and player.is_crocodile() and not jumped:
		jumped = true
		player.break_unfrozen()"

